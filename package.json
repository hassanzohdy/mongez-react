    "test": "tsc publisher.ts",
    "goes": "yarn build && mv package.json package.tmp.json && mv package.go.json package.json && npm version patch && cp package.json ./dist/package.json && mv package.json ./package.go.json && mv package.tmp.json package.json",
    "go": "yarn build && rm -rf ./publish && mkdir publish && cp -r ./dist/* publish && cd publishing-files && yarn version --new-version patch && cd .. && cp -r ./publishing-files/* ./publish && yarn publish ./publish --non-interactive"
